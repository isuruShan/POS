<p id="notice"><%= notice %></p>

<h1>Pos Usr Lorries</h1>

<table>
  <thead>
    <tr>
      <th>Reg number</th>
      <th>Number</th>
      <th>Image</th>
      <th>Pos usr transpoter</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pos_usr_lorries.each do |pos_usr_lorry| %>
      <tr>
        <td><%= pos_usr_lorry.reg_number %></td>
        <td><%= pos_usr_lorry.number %></td>
        <td><%= pos_usr_lorry.image %></td>
        <td><%= pos_usr_lorry.pos_usr_transpoter %></td>
        <td><%= link_to 'Show', pos_usr_lorry %></td>
        <td><%= link_to 'Edit', edit_pos_usr_lorry_path(pos_usr_lorry) %></td>
        <td><%= link_to 'Destroy', pos_usr_lorry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

